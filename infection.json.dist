{
    "timeout": 2,
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "text": "php://stderr",
        "badge": {
            "branch": "master"
        }
    },
    "mutators": {
        "@default": true,
        "IdenticalEqual": false,
        "NotIdenticalNotEqual": false,
        "UnwrapArrayFilter": false,
        "UnwrapArrayValues": false
    },
    "minMsi": 99,
    "minCoveredMsi": 100
}
